{
  "name": "action-scheduler-admin",
  "version": "0.1.0",
  "description": "Action Scheduler extension for WooCommerce Admin",
  "main": "index.js",
  "author": "Prospress",
  "License": "GPL-3.0",
  "scripts": {
    "install-if-deps-outdated": "node bin/install-if-deps-outdated.js",
    "install-if-no-packages": "node bin/install-if-no-packages.js",
    "clean": "rimraf ./dist",
    "prebuild": "npm run -s install-if-deps-outdated",
    "build:core": "npm run -s i18n && cross-env NODE_ENV=production webpack",
    "postbuild": "npm run -s i18n:php && npm run -s i18n:pot",
    "start": "webpack --watch",
    "i18n:js": "cross-env NODE_ENV=production babel js -o /dev/null",
    "i18n:php": "pot-to-php ./languages/action-scheduler-admin.pot ./languages/action-scheduler-admin.php action-scheduler-admin",
    "i18n:pot": "grunt makepot",
    "i18n:check": "grunt checktextdomain",
    "i18n": "npm run -s i18n:js && npm run -s i18n:php && npm run -s i18n:check && npm run -s i18n:pot",
    "lint:js": "npm run -s install-if-deps-outdated && eslint js --ext=js,jsx",
    "lint:php": "./vendor/bin/phpcs --standard=phpcs.xml.dist $(git ls-files | grep .php$)",
    "lint": "npm run -s lint:js && npm run -s lint:php"
  },
