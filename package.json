{
  "name": "action-scheduler-admin",
  "version": "0.1.0",
  "description": "Action Scheduler extension for WooCommerce Admin",
  "main": "index.js",
  "author": "Prospress",
  "License": "GPL-3.0",
  "scripts": {
    "install-if-deps-outdated": "node bin/install-if-deps-outdated.js",
    "install-if-no-packages": "node bin/install-if-no-packages.js",
    "clean": "rimraf ./dist",
    "prebuild": "npm run -s install-if-deps-outdated",
    "build": "npm run clean && npm run -s i18n && cross-env NODE_ENV=production webpack",
    "build:zip": "bin/build-zip.sh",
    "start": "webpack --watch",
    "i18n:js": "cross-env NODE_ENV=production babel js -o /dev/null",
    "i18n:pot": "wp --allow-root i18n make-pot . languages/action-scheduler-admin.pot --include=\"js,includes\" --exclude=\"dist,node_modules\" --slug=action-scheduler-admin",
    "i18n:check": "grunt checktextdomain",
    "i18n": "npm run -s i18n:js && npm run -s i18n:check && npm run -s i18n:pot",
    "lint:js": "npm run -s install-if-deps-outdated && eslint js --ext=js,jsx",
    "lint": "npm run -s lint:js"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@babel/plugin-transform-async-to-generator": "7.5.0",
    "@babel/plugin-transform-react-jsx": "7.3.0",
    "@babel/plugin-transform-typeof-symbol": "^7.7.4",
    "@babel/runtime-corejs2": "7.5.4",
    "@octokit/rest": "16.28.4",
    "@wordpress/babel-plugin-import-jsx-pragma": "1.1.3",
    "@wordpress/babel-plugin-makepot": "2.1.3",
    "@wordpress/babel-preset-default": "3.0.2",
    "@wordpress/browserslist-config": "2.4.0",
    "@wordpress/custom-templated-path-webpack-plugin": "1.3.0",
    "@wordpress/jest-preset-default": "4.1.0",
    "@wordpress/postcss-themes": "1.0.5",
    "ast-types": "0.13.2",
    "autoprefixer": "9.6.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-template-literals": "6.22.0",
    "chalk": "2.4.2",
    "color-studio": "1.0.5",
    "concurrently": "4.1.1",
    "copy-webpack-plugin": "5.0.3",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "deasync": "0.1.15",
    "deep-freeze": "0.0.1",
    "docsify-cli": "4.3.0",
    "eslint": "5.16.0",
    "eslint-config-wpcalypso": "4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-jest": "22.7.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.2",
    "eslint-plugin-wpcalypso": "4.1.0",
    "grunt": "1.0.4",
    "grunt-checktextdomain": "1.0.1",
    "grunt-wp-i18n": "1.0.3",
    "husky": "2.7.0",
    "lerna": "3.15.0",
    "locutus": "2.0.11",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "4.12.0",
    "node-watch": "0.6.2",
    "postcss-color-function": "4.1.0",
    "postcss-loader": "3.0.0",
    "prettier": "github:automattic/calypso-prettier#c56b4251",
    "promptly": "3.0.3",
    "prop-types": "15.7.2",
    "raw-loader": "1.0.0",
    "react-docgen": "2.21.0",
    "readline-sync": "1.4.9",
    "recast": "0.18.1",
    "replace": "1.1.0",
    "request-promise": "4.2.4",
    "rimraf": "2.6.3",
    "rtlcss": "2.4.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "9.10.1",
    "stylelint-config-wordpress": "13.1.0",
    "url-loader": "1.1.2",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.4",
    "webpack-rtl-plugin": "github:yoavf/webpack-rtl-plugin#develop"
  },
  "dependencies": {
    "@woocommerce/components": "^7.1.0",
    "@woocommerce/currency": "^3.2.0",
    "@woocommerce/navigation": "^6.1.0",
    "@wordpress/api-fetch": "2.2.8",
    "@wordpress/components": "17.0.0",
    "@wordpress/element": "^2.9.0",
    "@wordpress/hooks": "2.3.0",
    "@wordpress/i18n": "3.4.0",
    "@wordpress/url": "2.6.0"
  }
}
